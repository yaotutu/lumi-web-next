# 图片生成API配置 (二选一)
# 优先级: SILICONFLOW_API_KEY > ALIYUN_IMAGE_API_KEY

# SiliconFlow 文生图API (推荐)
SILICONFLOW_API_KEY=your_api_key_here
SILICONFLOW_API_ENDPOINT=https://api.siliconflow.cn/v1/images/generations
SILICONFLOW_IMAGE_MODEL=Qwen/Qwen-Image-Edit-2509

# 阿里云文生图API (备选)
ALIYUN_IMAGE_API_KEY=your_api_key_here
ALIYUN_IMAGE_API_ENDPOINT=https://dashscope.aliyuncs.com/api/v1/services/aigc/multimodal-generation/generation

# LLM API配置 - 用于提示词优化 (二选一)
# 优先级: SILICONFLOW_LLM_API_KEY > QWEN_API_KEY

# SiliconFlow LLM API (推荐)
SILICONFLOW_LLM_API_KEY=your_api_key_here
SILICONFLOW_LLM_BASE_URL=https://api.siliconflow.cn/v1
SILICONFLOW_LLM_MODEL=deepseek-ai/DeepSeek-V3

# 阿里云通义千问 (OpenAI兼容模式) (备选)
QWEN_API_KEY=sk-your-api-key-here
QWEN_BASE_URL=https://dashscope.aliyuncs.com/compatible-mode/v1
QWEN_MODEL=qwen-max

# 腾讯云图生3D API配置
TENCENT_SECRET_ID=your_secret_id_here
TENCENT_SECRET_KEY=your_secret_key_here
TENCENT_REGION=ap-guangzhou

# 开发环境配置
NEXT_PUBLIC_MOCK_MODE=true